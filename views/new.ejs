<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <% include ./partials/head.ejs %>
</head>
<body>
<div class="container">
 <% include ./partials/header.ejs %>
 <span>add a new practice</span>
 <div class="row">
 <div class="three columns">
    <form action="/dockit" method="POST">
    <span>distance</span><input type="number" name="distance"/><br>
    <span>zone</span><input type="number" name="zone"/><br>
    <span>water</span><input type="checkbox" name="onWater" id="onWater"/><br>
    <span>land</span><input type="checkbox"  name="onLand" id="onLand"/><br>
    </div>
    <div class="nine columns">
    <span>description</span><br><textarea placeholder="..." type="textarea" name="description"></textarea>
    </div>
    </div>
    <div class="button-primary bottom_btn">
    <a href="/dockit"><i class="fas fa-angle-double-left"></i></a>
    <input  type="submit" class="button-primary" name="" value="Submit"/>
    </div>
    </form>
    </div>
</body>
</html>